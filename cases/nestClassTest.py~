"""Parser for future statements

"""
class FutureParser:

    class BadFutureParser:
   
      def f():
        for x in range(3):
          if x < 3:
            print x
          else:
        print x+1

      def g(n):
         x = 0
         while x < n:
           if x < 3:
             print x
           elif x < 5:
             print x+3
           else:
             print x+1
         x += 1
         is_it_true = (3 * 4 > 10) or (5 + 5 > 10)
         print is_it_true

    def __init__(self):
        self.found = {} # set

